<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>

<VBox prefHeight="400.0" prefWidth="254.0"
	xmlns="http://javafx.com/javafx/8.0.172-ea"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="nl.bos.controllers.CheckinDialog"
	fx:id="vboxCheckinDialog">
	<HBox spacing="5.0">
		<TextField fx:id="filepath" editable="false"
			HBox.hgrow="ALWAYS" />
		<Button mnemonicParsing="false" onAction="#loadFile"
			text="Load" />
	</HBox>
	<Button fx:id="btnOK" defaultButton="true" disable="true"
		layoutX="60.0" layoutY="361.0" mnemonicParsing="false"
		onAction="#handleCheckin" prefWidth="65.0" text="OK" />
	<Button fx:id="btnCancel" cancelButton="true" layoutX="150.0"
		layoutY="361.0" mnemonicParsing="false" onAction="#handleExit"
		prefWidth="65.0" text="Cancel" />
</VBox>
